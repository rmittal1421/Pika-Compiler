func isEven<int i> -> bool {
	if((i/2)*2 == i) {
		return _true_.
	} else {
		return _false_.
	}
}

func parityString <int a> -> string {
	var numbers := [1,3,5,8,11,12].
	if((numbers map isEven)[3]) { return "even". }
	else { return "odd". }
}

exec {
	var numbers := [1,3,5,8,11,12].
	
	var parities := numbers map parityString.
	print parities.
	
